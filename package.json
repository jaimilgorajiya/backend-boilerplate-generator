{
  "name": "backend-boilerplate-generator",
  "displayName": "Backend Boilerplate Generator",
  "description": "Generate strict backend CRUD boilerplate for MERN stack",
  "publisher": "jaimilgorajiya",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaimilgorajiya/backend-boilerplate-generator"
  },
  "author": {
    "name": "Jaimil Gorajiya",
    "url": "https://github.com/jaimilgorajiya"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "backend-crud-helper.generateCrud",
        "title": "Backend Boilerplate Generator: Generate CRUD"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^18.0.0",
    "typescript": "^5.1.0"
  }
}
